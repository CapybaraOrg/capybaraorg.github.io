    "deploy": "gatsby build && gh-pages -d public -b main"
  },
  "dependencies": {
    "gatsby": "4.24.7",
    "react": "18.2.0",
    "react-dom": "18.2.0"
  },
  "devDependencies": {
    "gh-pages": "4.0.0",
    "prettier": "2.7.1"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/CapybaraOrg/capybaraorg.github.io"
  },
  "bugs": {
    "url": "https://github.com/CapybaraOrg/capybaraorg.github.io/issues"
  }
}
